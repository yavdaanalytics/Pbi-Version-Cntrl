			```
		formatString: 0
		lineageTag: 4b9d6391-4e5c-4067-990c-70e06039548e

	measure Customer_Revenue = ```
			
			CALCULATE(
			    SUM('Bigin - Deals Module'[Sum of Amount]),
			    'Bigin - Deals Module'[Package_Type] = "Suite",
			    'Bigin - Deals Module'[Stage] = "Closed Won"
			)
			
			```
		formatString: 0
		lineageTag: ea56ad70-8b71-480d-aecf-066a94addab3

	measure Product_Purchased_Count = ```
			
			CALCULATE (
			    COUNTROWS ( 'Bigin - Deals Module' ),
			    FILTER (
			        'Bigin - Deals Module',
			        'Bigin - Deals Module'[Visual_Purchased] = MAX ( 'Bigin - Deals Module'[Product_original_interest] )
			    )
			)
			
			```
		formatString: 0
		lineageTag: f36b16f9-4822-4a1b-9e39-886cd2112287

	measure Interested_Customers = ```
			
			VAR Product = SELECTEDVALUE('Bigin - Deals Module'[Product_original_interest])
			RETURN
			CALCULATE(
			    DISTINCTCOUNT('Bigin - Deals Module'[Account ID]),
			    'Bigin - Deals Module'[Product_original_interest] = Product
			)
			
			```
		formatString: 0
		lineageTag: 5915f628-b6ae-4b1a-8ae6-a539f6f430b5

	measure Bought_Customers = ```
			
			VAR Product = SELECTEDVALUE('Bigin - Deals Module'[Product_original_interest])
			
			VAR InterestedAccounts =
			    DISTINCT (
			        SELECTCOLUMNS(
			            FILTER(
			                'Bigin - Deals Module',
			                'Bigin - Deals Module'[Product_original_interest] = Product
			            ),
			            "Account", 'Bigin - Deals Module'[Account ID]
			        )
			    )
			
			VAR PurchasedAccounts =
			    DISTINCT (
			        SELECTCOLUMNS(
			            FILTER(
			                'Bigin - Deals Module',
			                'Bigin - Deals Module'[Visual_Purchased] = Product
			            ),
			            "Account", 'Bigin - Deals Module'[Account ID]
			        )
			    )
			
			VAR Intersection = INTERSECT(InterestedAccounts, PurchasedAccounts)
			
			RETURN
			COUNTROWS(Intersection)
			
			```
		formatString: 0
		lineageTag: 28dd552a-d190-4203-98d9-2efd01d348ef

	measure Interest_Conversion_Pct = ```
			
			DIVIDE( [Bought_Customers], [Interested_Customers], 0 )
			
			```
		lineageTag: b3e6d703-53ad-4815-86ac-c763c85eecf9

		annotation PBI_FormatHint = {"isGeneralNumber":true}

