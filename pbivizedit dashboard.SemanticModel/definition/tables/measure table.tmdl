	measure Closed_Lost_or_Expired = ```
			
			CALCULATE (
			    COUNTROWS ( 'Bigin - Deals Module' ),
			    'Bigin - Deals Module'[Stage] IN { "Closed Lost", "Closed Expired" }
			)
			
			```
		formatString: 0
		lineageTag: be650f5a-dbaf-4028-8326-1174034db227

	measure Avg_Closing_Days = ```
			
			AVERAGEX (
			    'Bigin - Deals Module',
			    DATEDIFF('Bigin - Deals Module'[Created Date], 'Bigin - Deals Module'[Closing_Date], DAY)
			)
			
			```
		lineageTag: bd1a5e08-afc6-4332-877f-0712c76c3020

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure Selected_KPI_Value = ```
			
			SWITCH (
			    SELECTEDVALUE ( KPI_Selector[KPI] ),
			    "Total Amount", [Total_Deal_Amount],
			    "Average Deal Size", AVERAGE ( 'Bigin - Deals Module'[Sum of Amount] ),
			    "Closed Lost Deals", [Closed_Lost_or_Expired],
			    "Average Closing Days", [Avg_Closing_Days],
			    "Closed Won Count" ,[Count_closed_won]
			)
			
			```
		lineageTag: 21e4a318-8167-4028-9cd0-147c485ea536

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure LineChart_Title = ```
			
